#
# Generic Cooling Fan status.
# Unknown fields should be set to NAN.
#



uint5 fan_index             # zero is first fan. 
int18 rpm                   # Negative value indicates reverse rotation
uint7 power_pct      # Instant demand factor in percent (percent of maximum power); range 0% to 127%.

uint8 ERROR_FLAG_HEALTHY  = 1
uint8 ERROR_FLAG_WARNING = 2
uint8 ERROR_FLAG_ERROR  = 4
uint8 health



