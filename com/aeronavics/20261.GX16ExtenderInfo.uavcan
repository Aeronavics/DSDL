uint8    CONTROLCMD_RUNCAN                      = 0x10   # RUN - CAN
uint8    CONTROLCMD_RUNCOM                      = 0x20   # RUN - COM
uint8    CONTROLCMD_RUNREMOTE                      = 0x30   # RUN - REMOTE

