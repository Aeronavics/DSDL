uint8    EXTENDER_WORKING_STATE_CRANK         = 0x40      # 01 - cranking
uint8    EXTENDER_WORKING_STATE_IDLE         = 0x80      # 10 - IDLE
uint8    EXTENDER_WORKING_STATE_RUN           = 0xc0      # 11 - running
uint8    WorkingState                                  # Engine working status(0~4) 

uint8     ECT1						# Engine coolant temperature 1 Engine temperature, unit: degree, rangge -40~215 degree; Offset:-40 degree
